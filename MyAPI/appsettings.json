{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
        "Microsoft.AspNetCore.Mvc.Razor": "Error",
        "Default": "Information"
      }
    },
    "NlogFile": {
      "Name": "allfile",
      "FileName": "c:/aplog/nlog-all-${shortdate}.log",
      "layout": "[${longdate}][${machinename}][${level}] ${message} ${exception}",
      "MaxArchiveFiles": 10,
      "ArchiveFileName": "ArchiveLog.{#}.log",
      "ArchiveNumbering": "Date",
      "ArchiveEvery": "Day",
      "ArchiveDateFormat": "yyyyMMdd",
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "NlogDB": {
      "Name": "SQLServer",
      "connectionStringName": "Server=.;Database=DemoQueryStore;Trusted_Connection=True;",
      "commandType": "StoredProcedure",
      "commandText": "[dbo].[usp_AddLog]"
    },
    "LogLevel": {
      "Default": "Debug",
      "MyAPI": "Trace",
      "System": "Error",
      "Microsoft": "Error"
    }
  },
  "ConnectionStrings": {
    "LoggerDatabase": "Server=.;Database=DemoQueryStore;Trusted_Connection=True;",
    "EnableCoreProfiler": true
  }
}
