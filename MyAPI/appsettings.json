{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
        "Microsoft.AspNetCore.Mvc.Razor": "Error",
        "Default": "Information"
      }
    },
    "LogLevel": {
      "Default": "Debug",
      "MyAPI": "Trace",
      "System": "Error",
      "Microsoft": "Error"
    }
  },
  "ConnectionStrings": {
    "LoggerDatabase": "Server=.;Database=DemoQueryStore;Trusted_Connection=True;",
    "EnableCoreProfiler": true
  }
}
